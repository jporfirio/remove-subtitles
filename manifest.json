{
  "manifest_version": 2,

  "name": "Remove subtitles",
  "description": "This extension completely hides subtitles from Netflix",
  "version": "0.1",
  "background": {"scripts": ["background.js"]},

  "browser_action": {
  },

  "permissions": [
    "activeTab", "tabs",
    "https://www.netflix.com/"
  ]
}
